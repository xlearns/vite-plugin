{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"compilerOptions": {
		"module": "ESNext",
		"target": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"declaration": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"outDir": "dist",
		"sourceMap": false,
		"noEmitOnError": true,
		"noImplicitAny": false
	}
}
